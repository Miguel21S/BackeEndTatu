{
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.2.1",
    "save": "^2.9.0",
    "typeorm": "^0.3.20"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ./src/server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "migrations-save": "typeorm-ts-node-commonjs migration:run -d ./src/database/db.ts",
    "revert-migrations": "npx typeorm-ts-node-commonjs migration:revert -d ./src/database/db.ts",
    "seed-roles": "ts-node ./src/database/seeders/seederRoles.ts",
    "seed-services": "ts-node ./src/database/seeders/seederServices.ts",
    "seed-users": "ts-node ./src/database/seeders/seederUsers.ts",
    "seed-appoits": "ts-node ./src/database/seeders/seederAppoitments.ts",
    "npx-create-table": "typeorm migration:create ./src/database/migrations/nombre de la tabla"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.20",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
